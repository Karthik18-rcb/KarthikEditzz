  <style>  
    * {  
      margin: 0;  
      padding: 0;  
      box-sizing: border-box;  
    }  
    body {  
      font-family: 'Montserrat', sans-serif;  
      background: #0f0f0f;  
      color: #e0e0e0;  
    }  
    header {  
      background: #1c1c1c;  
      text-align: center;  
      padding: 2rem;  
    }  
    header img {  
      max-height: 400px;  
      margin-bottom: 4rem;  
    }  
    header h1 {  
      font-size: 2.5rem;  
      color: #a86c2f;  
      letter-spacing: 2px;  
      margin-bottom: 0.5rem;  
    }  
    nav {  
      background: #121212;  
      display: flex;  
      justify-content: center;  
      gap: 2rem;  
      padding: 1rem;  
    }  
    nav a {  
      color: #e0e0e0;  
      text-decoration: none;  
      font-weight: 600;  
      transition: color 0.3s;  
    }  
    nav a:hover {  
      color: #a86c2f;  
    }  
    section {  
      padding: 2rem 1rem;  
      max-width: 900px;  
      margin: auto;  
      animation: fadeIn 0.8s ease-in;  
    }  
    @keyframes fadeIn {  
      from { opacity: 0; transform: translateY(10px); }  
      to { opacity: 1; transform: translateY(0); }  
    }  
    .reel-container {  
      display: grid;  
      grid-template-columns: 1fr 1fr;  
      gap: 1rem;  
    }  
    iframe {  
      width: 100%;  
      aspect-ratio: 9 / 16;  
      border: none;  
      border-radius: 10px;  
    }  
    footer {  
      background: #121212;  
      color: #999;  
      text-align: center;  
      padding: 1rem;  
    }  
    @media (max-width: 768px) {  
      .reel-container {  
        grid-template-columns: 1fr;  
      }  
    }  
  </style>  
</head>  
